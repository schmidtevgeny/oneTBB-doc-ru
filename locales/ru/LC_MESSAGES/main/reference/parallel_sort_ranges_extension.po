"namespace oneapi {\n"
"    namespace tbb {\n"
"\n"
"        template <typename Container>\n"
"        void parallel_sort( Container&& c );\n"
"        template <typename Container, typename Compare>\n"
"        void parallel_sort( Container&& c, const Compare& comp );\n"
"\n"
"    } // namespace tbb\n"
"} // namespace oneapi"
