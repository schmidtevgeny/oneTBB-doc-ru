msgid "This interface may be used for sorting rvalue or constant views:"
msgstr ""

#: ../../oneTBB/doc/main/reference/parallel_sort_ranges_extension.rst:59
msgid ""
"#include <array>\n"
"#include <span> // requires C++20\n"
"#include <oneapi/tbb/parallel_sort.h>\n"
"\n"
"std::span<int> get_span() {\n"
"    static std::array<int, 3> arr = {3, 2, 1};\n"
"    return std::span<int>(arr);\n"
"}\n"
"\n"
"int main() {\n"
"    tbb::parallel_sort(get_span());\n"
"}"
